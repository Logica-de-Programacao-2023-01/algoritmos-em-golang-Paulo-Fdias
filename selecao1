package main

import "fmt"

func main() {
	var n1 int
	var n2 int
	fmt.Print("Diga um número:")
	fmt.Scan(&n1)
	fmt.Print("Diga outro número:")
	fmt.Scan(&n2)
	x := n1
	y := n2

	if x > y {

		fmt.Println("x é maior que y")

	}
	if y > x {
		fmt.Println("y é maior que x")

	}
	if x == y {
		fmt.Println("x e y são iguais")
